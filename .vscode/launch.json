{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/MyStringVector.exe",  // 중복되지 않도록 확인
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",  // 중복되지 않도록 확인
            "environment": [],
            "externalConsole": true,
            "MIMode": "gdb",
            "miDebuggerPath": "C:/mingw/bin/gdb.exe",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "C/C++: g++.exe build active file",  // 중복되지 않도록 확인
            "internalConsoleOptions": "openOnSessionStart"
        }
    ]
}
